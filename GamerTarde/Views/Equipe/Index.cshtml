@model Equipe

@{
    ViewData["Title"] = "Equipe";
}

<form method="file" action='@Url.Action("Cadastrar")' enctype="multipart/form-data">

    x
    <input type="text" name="Nome" required>
    <input type="text" name="Imagem" required>

    <label for="">Imagem</label>
    <button type="submit">Cadastrar</button>

</form>

<h1>Tela de Equipes</h1>

<table class="table table-striped table-responsive">
    <thead>
        <th>Id</th>
        <th>Nome</th>
        <th>Imagem</th>
    </thead>
    <tbody>




        @foreach (Equipe equipe in ViewBag.Equipe)
        {
            <tr>
                <td>@equipe.IdEquipe</td>
                <td>@equipe.Nome</td>
                <td>@equipe.Imagem</td>
            </tr>
        }
    </tbody>
</table>